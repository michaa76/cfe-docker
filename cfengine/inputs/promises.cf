body common control
{
  inputs => {
    "$(sys.local_libdir)/files.cf",
    "$(sys.local_libdir)/common.cf",
    "ssh.cf",
    "processes.cf"
  };

  bundlesequence => {
      "ssh",
      "processes"
  };
}

body executor control
{
  exec_command => "/var/cfengine/bin/cf-agent";
  schedule => {
      "any"
  };  # minute resolution
}
